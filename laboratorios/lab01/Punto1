# ST0245-033
public class Counter
  {
     private String id;
     private int contador;
      
     public Counter(String id){
         this.id = id;
         contador = 1;
     }
      
     public void incrementar(){
          contador++;
     }
      
     public int incrementos(){
         return contador;
     }
      
     public String toString(){
        return "El contador qued√≥ en: " + contador;
     }
}
